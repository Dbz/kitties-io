<div class='panel panel-default favorite-item'>
	<div class='panel-heading'>
		<a href='<%= featuredProduct.url() %>'>
			<img class="favorite-item-img" src='<%= featuredProduct.images().first().get('url') %>'>
		</a>
	</div>
				
	<div class='panel-body product-link'>
		<a href="<% featuredProduct.url() %>"> <%= featuredProduct.get('name').substring(0, 35) %></a>
		<div class='product-money'>
			<a href="" class='product-price no-underline'> $<%= featuredProduct.get('price') %>K </a>
			<a href="" class='product-currency no-underline'> USD </a>
		</div>
	</div>
				
	<div class='panel-body'>
		<div class='row'>
			<% products.forEach(function(product){ %>
				<div class='col-md-4'>
						<a href='<%= product.url() %>'> <img class='other-seller-product' src='<%= product.images().first().get('url') %>'> </a>
				</div>
			<% }) %>
			<div class='col-md-4'>
				<div class='other-seller-product'>
				</div>
			</div>
		</div>
	</div>
							
	<div class='panel-footer'>
		<div class='row'>
			<div class='col-md-4'>
				<div class='seller-image'>
					<img class='img-circle'>
				</div>
			</div>
			<div class='col-md-8'>
				<div class='seller-bio'>
					<div class='seller-location'>
						<p> San Fransisco, California </p>
					</div>
					<div class='seller-rating'>
						<div class='review-stars'>
						</div>
						<p>☆☆☆☆☆ 48 reviews</p>
					</div>
				</div>
			</div>
		</div>
	</div>
</div>